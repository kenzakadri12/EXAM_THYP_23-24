# Définition des classes
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <#> .

:Sportif a rdf:Class ;
    rdfs:label "Sportif" .

:JeuxOlympiques a rdf:Class ;
    rdfs:label "Jeux Olympiques" .

:Epreuve a rdf:Class ;
    rdfs:label "Épreuve" .

:Resultat a rdf:Class ;
    rdfs:label "Résultat" .

# Définition des propriétés
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <#> .

:estInscritÀ rdf:property ;
    rdfs:label "est inscrit à" ;
    rdfs:domain :Sportif ;
    rdfs:range :JeuxOlympiques .

:aParticipéÀ rdf:property ;
    rdfs:label "a participé à" ;
    rdfs:domain :Sportif ;
    rdfs:range :Epreuve .

:aObtenu rdf:property ;
    rdfs:label "a obtenu" ;
    rdfs:domain :Sportif ;
    rdfs:range :Resultat .

:aEpreuve rdf:property ;
    rdfs:label "a épreuve" ;
    rdfs:domain :Resultat ;
    rdfs:range :Epreuve .

:aGagné rdf:property ;
    rdfs:label "a gagné" ;
    rdfs:domain :Resultat ;
    rdfs:range xsd:boolean .
